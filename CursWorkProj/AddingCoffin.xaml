<Window x:Class="CursWorkProj.AddingCoffin"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:CursWorkProj"
        mc:Ignorable="d"
        Title="Добавление нового гроба" Height="160" Width="320">
    <Grid>
        <Label Content="Добовление новго материала для гроба:" HorizontalAlignment="Center" Margin="0,20,0,0" VerticalAlignment="Top"/>
        <Label Content="Название материала:" HorizontalAlignment="Left" Margin="29,51,0,0" VerticalAlignment="Top"/>
        <TextBox x:Name="nameInput" HorizontalAlignment="Left" Margin="160,55,0,0" Text="" TextWrapping="Wrap" VerticalAlignment="Top" Width="128" Height="22"/>
        <Button x:Name="addButton" Content="Добавить" HorizontalAlignment="Center" Margin="0,92,0,0" VerticalAlignment="Top" Width="252" Height="32" Click="addButton_Click"/>
    </Grid>
</Window>
